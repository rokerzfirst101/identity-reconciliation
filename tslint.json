{
    "extends": "tslint:recommended",   
    "rulesDirectory": [],
    "rules": {
        "max-line-length": {
            "options": [120]
        },
        "no-arg": true,
        "no-console": {
            "severity": "warning",
            "options": ["debug", "info", "log", "time", "timeEnd", "trace"]
        }
    },
    "jsRules": {
        "max-line-length": {
            "options": [120]
        }
    }
}